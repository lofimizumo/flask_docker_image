{
  "apiVersion": "v1",
  "image": "bellakira0717/flask-battery-scheduler:latest",
  "name": "battery-optimization-ai",
  "enabled": true,
  "resources": {
    "machineType": "C5",
    "replicas": 1,
    "ports": [
      5000
    ]
  },
  "env": [
    {
      "name": "API_AMBER_QLD",
      "value": "secret:API_AMBER_QLD"
    },
    {
      "name": "API_AMBER_NSW",
      "value": "secret:API_AMBER_NSW"
    }
  ]
}